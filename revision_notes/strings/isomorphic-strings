good one do again
https://leetcode.com/problems/isomorphic-strings/submissions/1817314914/
//all we nwwd to check is whether a char ina  strign s is already matched to a character in t ,if yes, then is the matched character same as acurr char int t[i],if not return false
do it for s and also for t,since they asked to check both strings

my approach
class Solution {
public:
    bool isIsomorphic(string s, string t) {
        unordered_map<char,char>s_map,t_map;
        int n=s.size();
        if(n!=t.size())
        return false;
        
           for(int i=0;i<n;i++)
        {
            if(s_map.find(s[i])==s_map.end())
            {
                s_map[s[i]]=t[i];
            }
            else 
            {
                if(s_map[s[i]]!=t[i])
                return false;

            }
          
        }
          for(int i=0;i<n;i++)
        {
            if(t_map.find(t[i])==t_map.end())
            {
                t_map[t[i]]=s[i];
            }
            else 
            {
                if(t_map[t[i]]!=s[i])
                return false;

            }
          
        }
        return true;
    }
};
